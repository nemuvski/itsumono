{
  "name": "@itsumono/react",
  "version": "1.1.1",
  "author": "K.Utsunomiya",
  "description": "React.jsに関するユーティリティ関数やコンポーネントを提供する。",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://itsumono.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemuvski/itsumono.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/nemuvski/itsumono/issues"
  },
  "keywords": ["typescript", "utilities", "react"],
  "files": ["dist/**"],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --coverage",
    "build": "tsup src/ --format=esm,cjs --dts"
  },
  "devDependencies": {
    "@types/react": "^18.2.60",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": false,
    "minify": true,
    "clean": true
  },
  "typedocOptions": {
    "entryPoints": ["./src/index.ts"],
    "readme": "./README.md"
  }
}
