{
  "name": "itsumono",
  "author": "K.Utsunomiya",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=8.6"
  },
  "workspaces": [
    "packages/*",
    "website",
    "local/*"
  ],
  "scripts": {
    "lint": "biome lint ./",
    "lint:fix": "biome lint --apply ./",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build --filter=./packages/*",
    "docs": "npm run build -w website"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@local/tsconfig": "workspace:*",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.5.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "tsup": "^8.0.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
