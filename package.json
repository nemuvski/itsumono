{
  "name": "itsumono",
  "author": "K.Utsunomiya",
  "private": true,
  "packageManager": "pnpm@7.6.0",
  "engines": {
    "node": ">=16.14.0",
    "pnpm": ">=7.6"
  },
  "workspaces": [
    "packages/*",
    "website"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./packages",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./packages",
    "format": "prettier --check 'packages/**/*.{js,jsx,ts,tsx}'",
    "format:fix": "prettier --write 'packages/**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prebuild": "npm run typecheck && npm run lint && npm run format",
    "build": "turbo run build --filter=./packages/*",
    "docs": "npm run gen -w website"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.9.0",
    "expect-type": "^0.13.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "turbo": "^1.4.3",
    "typescript": "^4.8.3"
  }
}
